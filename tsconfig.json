{
  "files": [],
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "Components/*": ["Components/*"],
      "Pages/*": ["Pages/*"],
      "Assets/*": ["Assets/*"],
      "Interfaces/*": ["Interfaces/*"]
    },
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "references": [
    { "path": "./tsconfig.app.json" },
    { "path": "./tsconfig.node.json" }
  ]
}
